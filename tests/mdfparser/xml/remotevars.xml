        </action>
        
    </dmatrix>
    
    
    <events>
    
      <event class="0x00f" type="0x06" >
        <name lang="en">Counter data</name>                     
        <description lang="en">
          Count data steam event sent on regular intervals if activated. Coding: Integer. Unit: none.  
        </description>  
        <priority>4</priority>
        <data offset="0" >                      
          <name lang="en">Datacoding</name>
          <description lang="en">
            Will contain 0b01100xxx where xxx is the counter (0-3/4-7) and
            where 0-3 is the counter 32-bit value and 4-7 is the 32-bits of the
            64-bit counter value stored internally.
          </description>          
        </data>
        <data offset="1" >                      
          <name lang="en">MSB of counter</name>
          <description lang="en">
          Byte 0 (MSB) of 32-bit counter value.
          </description>          
        </data>
          <data offset="2" >
          <name lang="en">Counter value</name>
          <description lang="en">
          Byte 1 of 32-bit counter value.
          </description>          
        </data>
        <data offset="3" >
          <name lang="en">Counter value</name>
          <description lang="en">
          Byte 2 of 32-bit counter value.
          </description>          
        </data>
        <data offset="4" >
          <name lang="en">Counter value</name>
          <description lang="en">
          Byte 3 (LSB) of 32-bit counter value.
          </description>          
        </data>
      </event>
      
      <event class="0x001" type="0x01" >
        <name lang="en">Alarm occured</name>                    
        <description lang="en">
        If an alarm is armed this event is sent when it occurs and the corresponding 
        alarm bit is set in the alarm register.
        </description>  
        <priority>4</priority>
        <data offset="0" >                      
          <name lang="en">Index</name>
          <description lang="en">
          Counter alarm has occured on (0-3).
          </description>          
        </data>
        <data offset="1" >                      
          <name lang="en">Zone</name>
          <description lang="en">
          Is set to the zone for the module.
          </description>          
        </data>
        <data offset="2" >
          <name lang="en">Sub zone</name>
          <description lang="en">
          Sub zone for channel or sub zone for module if channel sub zone is zero.
          </description>          
        </data>
      </event>    

      <event class="0x00A" type="0x09" >
        <name lang="en">Frequency measurement</name>                    
        <description lang="en">
        Frequency measurement event sent on regular intervals if activated. 
        Coding: 32-bit integer. Unit: Hertz.
        </description>  
        <priority>4</priority>
        <data offset="0" >                      
          <name lang="en">Data coding</name>
          <description lang="en">
          Datacoding: 0b01100xxx where xxx is the counter (0-3)
          </description>          
        </data>
        <data offset="1" >                      
          <name lang="en">Frequency 32-bit value (MSB)</name>
          <description lang="en">
          Frequency value as 32-bit floating point value with MSB in first byte.
          </description>          
        </data>
        <data offset="2" >                      
          <name lang="en">Frequency 32-bit value</name>
          <description lang="en">
          Frequency value as 32-bit floating point value with MSB in first byte.
          </description>          
        </data>
        <data offset="3" >                      
          <name lang="en">Frequency 32-bit value</name>
          <description lang="en">
          Frequency value as 32-bit floating point value with MSB in first byte.
          </description>          
        </data>
        <data offset="4" >                      
          <name lang="en">Frequency 32-bit value (LSB)</name>
          <description lang="en">
          Frequency value as 32-bit floating point value with MSB in first byte.
          </description>          
        </data>
      </event>
      
      <event class="0x00A" type="-" >
        <name lang="en">Measurement Event</name>                    
        <description lang="en">
        Measurement values of all kinds can be sent as a result of linearization. 
        For example can an input counting S0 pulses be translated to KWh and watt  
        </description>  
        <priority>4</priority>
        <data offset="0" >                      
          <name lang="en">Datacoding</name>
          <description lang="en">
          Datacoding: Always as a 32-bit floating point value. 0b101yyxxx where yy is 
          the unit and xxx is the counter (0-3)
          </description>          
        </data>
        <data offset="1" >                      
          <name lang="en">32-bit floating point value (MSB)</name>
          <description lang="en">
          MSB of the 32-bit floating point value that has been calculated from 
          the linearization equation.
          </description>          
        </data>
        <data offset="2" >                      
          <name lang="en">32-bit floating point value</name>
          <description lang="en">
          32-bit floating point value that has been calculated from 
          the linearization equation.
          </description>          
        </data>
        <data offset="3" >                      
          <name lang="en">32-bit floating point value</name>
          <description lang="en">
          32-bit floating point value that has been calculated from 
          the linearization equation.
          </description>          
        </data>
        <data offset="4" >                      
          <name lang="en">32-bit floating point value (LSB)</name>
          <description lang="en">
          LSB of the 32-bit floating point value that has been calculated from 
          the linearization equation.
          </description>          
        </data>
      </event>
        
    </events>
        
</module>   
